{
  "name": "woodpecker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cd ../ && make generate-docs && cd docs && docusaurus start",
    "build": "pnpm build:woodpecker-plugins && docusaurus build",
    "build:woodpecker-plugins": "cd plugins/woodpecker-plugins && pnpm i && pnpm build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "format": "prettier --write .",
    "format:check": "prettier -c ."
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/faster": "^3.9.1",
    "@docusaurus/plugin-content-blog": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redocusaurus": "^2.5.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "^3.9.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.2.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
